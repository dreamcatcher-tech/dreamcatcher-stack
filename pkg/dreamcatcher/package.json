    "assert-fast": "^1.0.3",
    "ethers": "^6.6.4",
    "fast-deep-equal": "^3.1.3",
    "jimp": "^0.22.8",
    "json-schema-faker": "^0.5.3",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.8.1",
