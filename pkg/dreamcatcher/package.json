  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "maxWorkers": "95%",
    "testTimeout": 300
  },
  "wallaby": {
    "autoDetect": true,
    "tests": [
      "**/*.test.js"
    ],
    "env": {
      "type": "node",
      "runner": "node",
      "params": {
        "runner": "--experimental-vm-modules"
      }
    }
